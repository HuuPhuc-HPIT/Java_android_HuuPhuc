package hpit.com;

public class TestFor_Img1 {
	
	public static void hinh1(int h) {
		for(int i=0; i<h; i++) {
			for(int j=i; j<h; j++) {
				System.out.print("*");
			}
			System.out.println();
		}
	}
	
	public static void hinh2(int h) {
		for(int i=0; i<h; i++) {
			
			for(int j=h; j<h; j--) {
				System.out.print("*");				
			}
			System.out.println();
		}
	}
	public static void main(String[] args) {
		hinh2(5);
	}

}
