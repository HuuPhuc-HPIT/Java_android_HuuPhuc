package hpit.com;

import java.util.Scanner;

public class Bt2SoHoanThien {

	public static void main(String[] args) {
		Scanner sc= new Scanner(System.in);
		System.out.println("Nhap vao so n: ");
		int n= sc.nextInt();
		
		while(n<0) {
			System.out.println("Nhap lai so n: ");
			n= sc.nextInt();
		}
	}

}
